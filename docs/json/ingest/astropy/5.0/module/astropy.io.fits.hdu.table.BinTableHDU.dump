      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The primary use for the "
                }
              },
              {
                "type": "Link",
                "data": {
                  "value": "dump",
                  "reference": {
                    "module": "astropy",
                    "version": "5.0",
                    "kind": "module",
