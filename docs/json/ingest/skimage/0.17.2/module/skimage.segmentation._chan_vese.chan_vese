            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "'difference from average' weight parameter for the output region with value 'True'. If it is lower than "
                      }
                    },
                    {
                      "type": "Link",
                      "data": {
                        "value": "lambda2",
                        "reference": {
                          "module": null,
                          "version": null,
