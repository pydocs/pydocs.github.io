    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Perform a projective transformation (homography) of a floating point image (single or double precision), using interpolation."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "For each pixel, given its homogeneous coordinate "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "\\mathbf{x}\n= [x, y, 1]^T"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", its target position is calculated by multiplying with the given matrix, "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "H"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", to give "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "H \\mathbf{x}"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". E.g., to rotate by theta degrees clockwise, the matrix should be::    "
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "[[cos(theta) -sin(theta) 0]",
                  "_number": 0,
                  "_offset": 0
                },
                {
                  "_line": " [sin(theta)  cos(theta) 0]",
                  "_number": 1,
                  "_offset": 0
                },
                {
                  "_line": " [0            0         1]]",
                  "_number": 2,
                  "_offset": 0
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "or, to translate x by 10 and y by 20::    "
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "[[1 0 10]",
                  "_number": 0,
                  "_offset": 0
                },
                {
                  "_line": " [0 1 20]",
                  "_number": 1,
                  "_offset": 0
                },
                {
                  "_line": " [0 0 1 ]].",
                  "_number": 2,
                  "_offset": 0
                }
              ]
            }
          }
        }
      ],
      "title": null
