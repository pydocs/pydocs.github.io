                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "A binary image containing the objects to be skeletonized. Zeros represent background, nonzero values are foreground."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "method",
