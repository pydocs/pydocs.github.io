          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Thinning is used to reduce each connected component in a binary image to a single-pixel wide skeleton."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
