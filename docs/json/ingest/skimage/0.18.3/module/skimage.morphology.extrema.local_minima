          "data": {
            "value": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Perform a flood-fill to find all connected pixels that have the same    gray value and are part of the plateau."
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Consider the connected neighborhood of a plateau: if no bordering sample    has a smaller gray level, mark the plateau as a definite local minimum."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
