                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "\"Registered options\" and \"Deprecated options\" have metadata associated   with them, which are stored in auxiliary dictionaries keyed on the   fully-qualified key, e.g. \"x.y.z.option\"."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
