        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "minimize f(x) subject to",
                  "_number": 0,
                  "_offset": 0
                },
                {
                  "_line": "",
                  "_number": 1,
                  "_offset": 0
                },
                {
                  "_line": "g_i(x) >= 0,  i = 1,...,m",
                  "_number": 2,
                  "_offset": 0
                },
                {
                  "_line": "h_j(x)  = 0,  j = 1,...,p",
                  "_number": 3,
                  "_offset": 0
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "where x is a vector of one or more variables. "
