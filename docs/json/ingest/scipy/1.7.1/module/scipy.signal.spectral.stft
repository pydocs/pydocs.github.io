              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Given a time-domain signal "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "x[n]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", a window "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "w[n]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", and a hop size "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "H"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " = "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": "nperseg - noverlap",
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", the windowed frame at time index "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "t"
                  ]
