              {
                "type": "Math",
                "data": {
                  "value": [
                    "J_\\mu"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the Bessel function of order "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "\\mu"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ".  The index "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "\\mu"
                  ]
                }
              },
              {
                "type": "Words",
