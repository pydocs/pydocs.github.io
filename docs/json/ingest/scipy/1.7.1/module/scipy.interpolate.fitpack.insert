                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "A 1-D point at which to insert a new knot(s).  If "
                      }
                    },
                    {
                      "type": "Link",
                      "data": {
                        "value": "tck",
                        "reference": {
                          "module": null,
                          "version": null,
                          "kind": "local",
