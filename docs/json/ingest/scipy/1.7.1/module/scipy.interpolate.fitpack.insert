              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "If non-zero, the input spline is considered periodic."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        }
