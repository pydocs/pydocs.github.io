        {
          "type": "Param",
          "data": {
            "param": "lstsq",
            "type_": "bool",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "True if the system is ill-conditioned and/or (nearly) singular and thus a more robust least-squares solver is desired. This is sometimes needed as the solution is approached."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
