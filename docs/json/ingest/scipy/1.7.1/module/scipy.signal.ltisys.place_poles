                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "A - B*K"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " is compared to its previous value, when the relative error between these two values becomes lower than "
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": "rtol",
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
