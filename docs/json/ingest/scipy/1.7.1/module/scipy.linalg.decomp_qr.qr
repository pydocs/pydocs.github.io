      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Calculate the decomposition "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "A = Q R"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " where Q is unitary/orthogonal and R upper triangular."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
