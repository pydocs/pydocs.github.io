          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Returns a matrix containing the Cholesky decomposition, "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "A = L L*"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " or "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "A = U* U"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " of a Hermitian positive-definite matrix "
                }
              },
              {
                "type": "Link",
                "data": {
                  "value": "a",
                  "reference": {
                    "module": null,
                    "version": null,
                    "kind": "local",
                    "path": "a"
                  },
                  "kind": "local",
                  "exists": true
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". The return value can be directly used as the first parameter to cho_solve."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Admonition",
          "data": {
            "kind": "warning",
