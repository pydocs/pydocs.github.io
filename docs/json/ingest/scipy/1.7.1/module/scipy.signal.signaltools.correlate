      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The correlation z of two d-dimensional arrays x and y is defined as::      "
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "z[...,k,...] = sum[..., i_l, ...] x[..., i_l,...] * conj(y[..., i_l - k,...])",
                  "_number": 0,
                  "_offset": 0
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This way, if x and y are 1-D arrays and "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "z = correlate(x, y, 'full')"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " then"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockMath",
          "data": {
            "value": "z [ k] = ( x * y) ( k - N + 1) = \\sum_ { l=0}^ { ||x|| - 1}x_l y_ { l - k+N - 1}^ { *} "
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "for "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "k = 0, 1, ..., ||x|| + ||y|| - 2"
                  ]
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "where "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "||x||"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the length of "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "N = \\max(||x||,||y||)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", and "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "y_m"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is 0 when m is outside the range of y."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "method='fft'"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " only works for numerical arrays as it relies on "
                }
              },
              {
                "type": "Link",
