    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The observation equations are "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "A @ c = y"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", and the normal equations are "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "A.T @ A @ c = A.T @ y"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". This routine fills in the rhs and lhs for the latter."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The B-spline collocation matrix is defined as "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "A_{j,l} = B_l(x_j)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", so that row "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "j"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " contains all the B-splines which are non-zero at "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x_j"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The normal eq matrix has at most "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": "2k+1",
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " bands and is constructed in the LAPACK symmetrix banded storage: "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "A[i, j] == ab[i-j, j]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " with "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": "i >= j",
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". See the doctsring for "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": "scipy.linalg.cholesky_banded",
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " for more info."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This routine is not supposed to be called directly, and does no error checking."
                }
              }
            ],
            "inner": []
          }
        }
      ],
