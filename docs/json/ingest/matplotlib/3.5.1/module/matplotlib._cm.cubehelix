                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Gamma factor emphasizing either low intensity values (gamma < 1), or high intensity values (gamma > 1)."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
