                            "type": "Verbatim",
                            "data": {
                              "value": [
                                "%store"
                              ]
                            }
                          },
                          {
                            "type": "Words",
                            "data": {
                              "value": "          - Show list of all variables and their current"
                            }
                          }
                        ],
                        "inner": []
                      },
                      "dd": [
                        {
                          "type": "Paragraph",
                          "data": {
                            "inline": [
                              {
                                "type": "Words",
                                "data": {
                                  "value": "values"
                                }
                              }
                            ],
                            "inner": []
                          }
                        }
                      ]
                    }
                  ]
                }
              },
              {
                "type": "DefList",
                "data": {
                  "children": [
                    {
                      "dt": {
                        "inline": [
                          {
                            "type": "Verbatim",
                            "data": {
                              "value": [
                                "%store spam bar"
                              ]
                            }
                          },
                          {
                            "type": "Words",
                            "data": {
                              "value": " - Store the "
                            }
                          },
                          {
                            "type": "Emph",
                            "data": {
                              "value": {
                                "value": "current"
                              }
                            }
                          },
                          {
                            "type": "Words",
                            "data": {
                              "value": " value of the variables spam"
                            }
                          }
                        ],
                        "inner": []
                      },
                      "dd": [
                        {
                          "type": "Paragraph",
                          "data": {
                            "inline": [
                              {
                                "type": "Words",
                                "data": {
                                  "value": "and bar to disk"
                                }
                              }
                            ],
                            "inner": []
                          }
                        }
                      ]
                    }
                  ]
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "%store -d spam"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "  - Remove the variable and its value from storage"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "%store -z"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "       - Remove all variables from storage"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "DefList",
                "data": {
                  "children": [
                    {
                      "dt": {
                        "inline": [
                          {
                            "type": "Verbatim",
                            "data": {
                              "value": [
                                "%store -r"
                              ]
                            }
                          },
                          {
                            "type": "Words",
                            "data": {
                              "value": "       - Refresh all variables, aliases and directory history"
                            }
                          }
                        ],
                        "inner": []
                      },
                      "dd": [
                        {
                          "type": "Paragraph",
                          "data": {
                            "inline": [
                              {
                                "type": "Words",
                                "data": {
                                  "value": "from store (overwrite current vals)"
                                }
                              }
                            ],
                            "inner": []
                          }
                        }
                      ]
                    }
                  ]
                }
              },
              {
                "type": "DefList",
                "data": {
                  "children": [
                    {
                      "dt": {
                        "inline": [
                          {
                            "type": "Verbatim",
                            "data": {
                              "value": [
                                "%store -r spam bar"
                              ]
                            }
                          },
                          {
                            "type": "Words",
                            "data": {
                              "value": " - Refresh specified variables and aliases from store"
                            }
                          }
                        ],
                        "inner": []
                      },
                      "dd": [
                        {
                          "type": "Paragraph",
                          "data": {
                            "inline": [
                              {
                                "type": "Words",
                                "data": {
                                  "value": "(delete current val)"
                                }
                              }
                            ],
                            "inner": []
                          }
                        }
                      ]
                    }
                  ]
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "%store foo >a.txt"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "  - Store value of foo to new file a.txt"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "%store foo >>a.txt"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " - Append value of foo to file a.txt"
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "It should be noted that if you change the value of a variable, you need to %store it again if you want to persist the new value."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Note also that the variables will need to be pickleable; most basic python types can be safely %store'd."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Also aliases can be %store'd across sessions. To remove an alias from the storage, use the %unalias magic."
                }
              }
            ],
            "inner": []
          }
        }
      ],
