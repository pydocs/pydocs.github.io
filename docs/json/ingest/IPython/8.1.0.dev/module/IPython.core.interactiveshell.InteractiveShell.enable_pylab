            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This turns on support for matplotlib, preloads into the interactive namespace all of numpy and pylab, and configures IPython to correctly interact with the GUI event loop.  The GUI backend to be used can be optionally selected with the optional "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "gui"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " argument."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This method only adds preloading the namespace to InteractiveShell.enable_matplotlib."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
