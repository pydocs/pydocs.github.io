              {
                "type": "Words",
                "data": {
                  "value": "The screen_lines parameter specifies the number of "
                }
              },
              {
                "type": "Emph",
                "data": {
                  "value": {
                    "value": "usable"
                  }
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " lines of your terminal screen (total lines minus lines you need to reserve to show other information)."
