        },
        {
          "type": "EnumeratedList",
          "data": {
            "value": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "%sx differs from %sc in that %sx automatically splits into a list,"
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "like '%sc -l'.  The reason for this is to make it as easy as possible to process line-oriented shell output via further python commands. %sc is meant to provide much finer control, but requires more typing."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": ".l (or .list) : value as list.",
                  "_number": 3,
                  "_offset": 0
                },
                {
                  "_line": ".n (or .nlstr): value as newline-separated string.",
                  "_number": 4,
                  "_offset": 0
                },
                {
                  "_line": ".s (or .spstr): value as whitespace-separated string.",
                  "_number": 5,
                  "_offset": 0
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This is very useful when trying to use such lists as arguments to system commands."
                }
              }
            ],
            "inner": []
          }
        }
