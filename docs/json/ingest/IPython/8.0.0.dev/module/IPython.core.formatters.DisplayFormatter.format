        {
          "type": "BlockQuote",
          "data": {
            "value": [
              "If an object implement `_repr_mimebundle_` as well as various",
              "`_repr_*_`, the data returned by `_repr_mimebundle_` will take",
              "precedence and the corresponding `_repr_*_` for this mimetype will",
              "not be called."
            ]
          }
        }
      ],
      "title": null
