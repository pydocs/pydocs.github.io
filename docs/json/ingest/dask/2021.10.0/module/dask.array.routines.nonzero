          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "While the nonzero values can be obtained with "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "a[nonzero(a)]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", it is recommended to use "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x[x.astype(bool)]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " or "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x[x != 0]"
