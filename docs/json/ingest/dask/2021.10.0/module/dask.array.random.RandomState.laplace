          "data": {
            "param": "loc",
            "type_": "float or array_like of floats, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The position, "
                      }
                    },
                    {
                      "type": "Math",
                      "data": {
                        "value": [
                          "\\mu"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ", of the distribution peak. Default is 0."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "scale",
            "type_": "float or array_like of floats, optional",
            "desc": [
              {
                "type": "Paragraph",
