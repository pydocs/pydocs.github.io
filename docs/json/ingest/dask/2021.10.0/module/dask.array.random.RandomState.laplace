                  "inline": [
                    {
                      "type": "Math",
                      "data": {
                        "value": [
                          "\\lambda"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ", the exponential decay. Default is 1. Must be non- negative."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "size",
            "type_": "int or tuple of ints, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Output shape.  If the given shape is, e.g., "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "(m, n, k)"
                        ]
                      }
                    },
                    {
                      "type": "Words",
