      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "We share neighboring zones between blocks of the array, map a function, and then trim away the neighboring strips. If depth is larger than any chunk along a particular axis, then the array is rechunked."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Note that this function will attempt to automatically determine the output array type before computing it, please refer to the "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "meta"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " keyword argument in "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "map_blocks"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " if you expect that the function will not succeed when operating on 0-d arrays."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
