        {
          "type": "Param",
          "data": {
            "param": "func: function",
            "type_": "",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The function to apply to each extended block. If multiple arrays are provided, then the function should expect to receive chunks of each array in the same order."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "args",
            "type_": "dask arrays",
            "desc": []
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "depth: int, tuple, dict or list",
            "type_": "",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The number of elements that each block should share with its neighbors If a tuple or dict then this can be different per axis. If a list then each element of that list must be an int, tuple or dict defining depth for the corresponding array in "
                      }
                    },
                    {
                      "type": "Link",
                      "data": {
                        "value": "args",
                        "reference": {
                          "module": null,
                          "version": null,
                          "kind": "local",
                          "path": "args"
                        },
                        "kind": "local",
                        "exists": true
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ". Asymmetric depths may be specified using a dict value of (-/+) tuples. Note that asymmetric depths are currently only supported when "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "boundary"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " is 'none'. The default value is 0."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "boundary: str, tuple, dict or list",
            "type_": "",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "How to handle the boundaries. Values include 'reflect', 'periodic', 'nearest', 'none', or any constant value like 0 or np.nan. If a list then each element must be a str, tuple or dict defining the boundary for the corresponding array in "
                      }
                    },
                    {
                      "type": "Link",
                      "data": {
                        "value": "args",
                        "reference": {
                          "module": null,
                          "version": null,
                          "kind": "local",
                          "path": "args"
                        },
                        "kind": "local",
                        "exists": true
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ". The default value is 'reflect'."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "trim: bool",
            "type_": "",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Whether or not to trim "
