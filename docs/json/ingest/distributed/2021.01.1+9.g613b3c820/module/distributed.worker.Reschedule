            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Raising this exception will stop the current execution of the task and ask the scheduler to reschedule this task, possibly on a different machine."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This does not guarantee that the task will move onto a different machine. The scheduler will proceed through its normal heuristics to determine the optimal machine to accept this task.  The machine will likely change if the load across the cluster has significantly changed since first scheduling the task."
                }
              }
            ],
            "inner": []
          }
        }
