        {
          "type": "Param",
          "data": {
            "param": "label",
            "type_": "{'fast', 'full', '', attribute identifier}, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Identifies the benchmarks to run. This can be a string to pass to the nosetests executable with the '-A' option, or one of several special values.  Special values are:"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "BulletList",
                "data": {
                  "value": [
                    {
                      "type": "Paragraph",
                      "data": {
                        "inline": [
                          {
                            "type": "Words",
                            "data": {
                              "value": "'fast' - the default - which corresponds to the "
                            }
                          },
                          {
                            "type": "Verbatim",
                            "data": {
                              "value": [
                                "nosetests -A"
                              ]
                            }
                          },
                          {
                            "type": "Words",
                            "data": {
                              "value": "   option of 'not slow'."
                            }
                          }
                        ],
                        "inner": []
                      }
                    },
                    {
                      "type": "Paragraph",
                      "data": {
                        "inline": [
                          {
                            "type": "Words",
                            "data": {
                              "value": "'full' - fast (as above) and slow benchmarks as in the   'no -A' option to nosetests - this is the same as ''."
                            }
                          }
                        ],
                        "inner": []
                      }
                    },
                    {
                      "type": "Paragraph",
                      "data": {
                        "inline": [
                          {
                            "type": "Words",
                            "data": {
                              "value": "None or '' - run all tests."
                            }
                          }
                        ],
                        "inner": []
                      }
                    },
                    {
                      "type": "Paragraph",
                      "data": {
                        "inline": [
                          {
                            "type": "Words",
                            "data": {
                              "value": "attribute_identifier - string passed directly to nosetests as '-A'."
                            }
                          }
                        ],
                        "inner": []
                      }
                    }
                  ]
                }
