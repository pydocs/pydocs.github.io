                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Only useful in forcing objects in object arrays on Python 3 to be pickled in a Python 2 compatible way. If "
                      }
                    },
                    {
                      "type": "Link",
                      "data": {
                        "value": "fix_imports",
