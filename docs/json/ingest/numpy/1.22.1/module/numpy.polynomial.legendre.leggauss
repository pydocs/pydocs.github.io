    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Computes the sample points and weights for Gauss-Legendre quadrature. These sample points and weights will correctly integrate polynomials of degree "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "2*deg - 1"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " or less over the interval "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "[-1, 1]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " with the weight function "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "f(x) = 1"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "type": "Admonition",
          "data": {
            "kind": "versionadded",
            "title": "TODO",
            "children": [
              {
                "inline": [
                  {
                    "type": "Words",
                    "data": {
                      "value": "1.7.0 "
                    }
                  }
                ],
                "inner": []
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The results have only been tested up to degree 100, higher degrees may be problematic. The weights are determined by using the fact that"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockMath",
          "data": {
            "value": "w_k = c / ( L ' _n ( x_k) * L_ { n - 1} ( x_k)) "
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "where "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "c"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is a constant independent of "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "k"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " and "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "x_k"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the k'th root of "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "L_n"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", and then scaling the results to get the right value when integrating 1."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
