                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "1D array of polynomial coefficients from highest to lowest degree:"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "c[0] * x**(N) + c[1] * x**(N-1) + ... + c[N-1] * x + c[N]"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " where c[0] always equals 1."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        }
      ],
