    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Covariance indicates the level to which two variables vary together. If we examine N-dimensional samples, "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "X = [x_1, x_2, ... x_N]^T"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", then the covariance matrix element "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "C_{ij}"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the covariance of "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "x_i"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " and "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "x_j"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". The element "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "C_{ii}"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the variance of "
                }
              },
              {
                "type": "Math",
                "data": {
                  "value": [
                    "x_i"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "See the notes for an outline of the algorithm."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
