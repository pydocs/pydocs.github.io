              {
                "type": "Words",
                "data": {
                  "value": "When order is 'A' and "
                }
              },
              {
                "type": "Link",
                "data": {
                  "value": "object",
                  "reference": {
                    "module": null,
                    "version": null,
                    "kind": "local",
